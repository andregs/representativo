{
  "name": "representativo",
  "description": "Aplicativo que visa ajudar os eleitores a escolherem melhor os seus candidatos.",
  "version": "0.0.0",
  "author": "André Gomes <andregs@gmail.com> (http://andregs.com/)",
  "license": "Apache-2.0",
  "angular-cli": {},
  "scripts": {
    "reinit": "ng init -sd client --style scss --routing true",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "nodemon": "nodemon -w dist -i dist/public --inspect",
    "dev": "npm-run-all clean -p build-watch test-watch",
    "clean": "rimraf dist coverage",
    "build": "npm-run-all test -p build:*",
    "build-watch": "run-p \"build:* -- -w\"",
    "build-prod": "npm-run-all clean test -p \"build:client -- -prod\" build:server",
    "build:client": "ng build -pr false",
    "build:server": "tsc -p server",
    "lint": "tslint server/**/*.ts client/**/*.ts",
    "test": "run-s lint \"test:client -- -w false\"",
    "test-watch": "run-p \"test:* -- --single-run=false\"",
    "test:client": "karma start karma.conf.js --angular-cli.codeCoverage=true",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "main": "dist/server.js",
  "homepage": "https://github.com/andregs/representativo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andregs/representativo.git"
  },
  "bugs": {
    "url": "https://github.com/andregs/representativo/issues"
  },
  "directories": {
    "doc": "doc"
  },
  "dependencies": {
    "@angular/common": "2.4.7",
    "@angular/compiler": "2.4.7",
    "@angular/compiler-cli": "2.4.7",
    "@angular/core": "2.4.7",
    "@angular/forms": "2.4.7",
    "@angular/http": "2.4.7",
    "@angular/platform-browser": "2.4.7",
    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/router": "3.4.7",
    "body-parser": "1.16.1",
    "core-js": "2.4.1",
    "express": "4.14.1",
    "rxjs": "5.1.0",
    "ts-helpers": "1.1.2",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.3.1",
    "@types/body-parser": "0.0.33",
    "@types/express": "4.0.35",
    "@types/jasmine": "2.5.42",
    "@types/node": "7.0.5",
    "angular-cli": "1.0.0-beta.28.3",
    "codelyzer": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-nightmare": "0.4.0",
    "karma-remap-istanbul": "0.6.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.1",
    "protractor": "5.1.1",
    "rimraf": "2.5.4",
    "ts-node": "2.1.0",
    "tslint": "4.4.2",
    "typescript": "2.1.6"
  },
  "keywords": [
    "política",
    "eleições",
    "eleição",
    "eleitor",
    "eleitores",
    "voto",
    "candidatos",
    "candidato",
    "partidos",
    "partidos políticos",
    "representatividade"
  ]
}

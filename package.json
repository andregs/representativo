{
  "name": "representativo",
  "description": "Aplicativo que visa ajudar os eleitores a escolherem melhor os seus candidatos.",
  "version": "0.0.0",
  "author": "André Gomes <andregs@gmail.com> (http://andregs.com/)",
  "license": "Apache-2.0",
  "angular-cli": {},
  "scripts": {
    "reinit": "ng init -p re -sd client --style scss --routing true",
    "db:create": "node script/create-db.script.js",
    "db:drop": "node script/drop-db.script.js",
    "db:recreate": "run-s db:drop db:create",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "nodemon": "nodemon -w dist -i dist/public --inspect",
    "dev": "npm-run-all clean -p build-watch test-watch",
    "tunnel": "lt --port 3000",
    "clean": "rimraf dist coverage",
    "build": "npm-run-all test -p build:*",
    "build-watch": "run-p \"build:* -- -w\"",
    "build-prod": "npm-run-all clean test -p \"build:client -- -prod\" build:server",
    "build:client": "ng build -pr false",
    "build:server": "tsc -p server",
    "lint": "tslint server/**/*.ts client/**/*.ts",
    "test": "run-s lint test:server test:client",
    "test-watch": "run-p \"test:client -- --single-run=false\" test:server:watch",
    "test:client": "karma start karma.conf.js --angular-cli.codeCoverage=true",
    "test:server": "ts-node -P server server/jasmine.js",
    "test:server:watch": "chokidar \"server/**\" -c \"npm run test:server\" --initial",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "main": "dist/server.js",
  "homepage": "https://github.com/andregs/representativo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andregs/representativo.git"
  },
  "bugs": {
    "url": "https://github.com/andregs/representativo/issues"
  },
  "directories": {
    "doc": "doc"
  },
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/compiler-cli": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "angular2-jwt": "0.1.28",
    "arangojs": "5.6.0",
    "auth0-lock": "10.11.0",
    "body-parser": "1.16.1",
    "core-js": "2.4.1",
    "express": "4.14.1",
    "express-jwt": "5.1.0",
    "rxjs": "5.1.1",
    "ts-helpers": "1.1.2",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.32.3",
    "@types/body-parser": "0.0.34",
    "@types/express": "4.0.35",
    "@types/express-jwt": "0.0.34",
    "@types/jasmine": "2.5.43",
    "@types/jsonwebtoken": "7.2.0",
    "@types/node": "7.0.5",
    "@types/supertest": "2.0.0",
    "cerialize": "0.1.14",
    "chokidar-cli": "1.2.0",
    "codelyzer": "2.0.0",
    "eslint": "3.15.0",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "jsonwebtoken": "7.3.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-nightmare": "0.4.2",
    "localtunnel": "1.8.2",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.1",
    "protractor": "5.1.1",
    "rimraf": "2.6.0",
    "supertest": "3.0.0",
    "ts-node": "2.1.0",
    "tslint": "4.4.2",
    "typescript": "2.1.6"
  },
  "keywords": [
    "política",
    "eleições",
    "eleição",
    "eleitor",
    "eleitores",
    "voto",
    "candidatos",
    "candidato",
    "partidos",
    "partidos políticos",
    "representatividade"
  ]
}

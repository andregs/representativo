{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "globals": {
    "auth0": false,
    "configuration": false,
    "jwt": false
  },
  "rules": {
    "no-console": "off"
  }
}
